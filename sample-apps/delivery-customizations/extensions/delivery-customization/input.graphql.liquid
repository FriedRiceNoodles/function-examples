query Input {
  cart {
    deliveryGroups {
      deliveryAddress {
        provinceCode
      }
      deliveryOptions {
        handle
        title
      }
    }
  }
  deliveryCustomization {
    metafield(
      namespace: "{{ app_name | replace: " ", "-" | downcase }}"
      key: "function-configuration"
    ) {
      value
    }
  }
}
