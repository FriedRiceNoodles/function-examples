import { describe, it, expect } from 'vitest';
import checkoutCompletionTargetCustomization from './index';

/**
 * @typedef {import("../generated/api").FunctionResult} FunctionResult
 */

describe('checkout completion target customization function', () => {
  it('returns no operations without configuration', () => {
    const result = checkoutCompletionTargetCustomization({
      checkoutCompletionTargetCustomization: {
        metafield: null
      }
    });
    const expected = /** @type {FunctionResult} */ ({ operations: [] });

    expect(result).toEqual(expected);
  });
});
